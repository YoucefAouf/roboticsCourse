<?xml version="1.0"?>
<robot name="three_wheeled_car">
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder length="0.024" radius="0.175"/>
            </geometry>
            <material name="red">
                <color rgba="0.8 0 0 0.5"/>
            </material>
        </visual>
    </link>

    <link name="base_footprint">
    </link>
    <joint name="footprint_to_base" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0 0 0.036" rpy="0 0 0"/>
    </joint>

    <link name="right_wheel">
        <visual>
            <geometry>
                <cylinder length="0.008" radius="0.036"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
    </link>

    <joint name="base_to_right_wheel" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0 -0.179 0" rpy="-1.57 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="left_wheel">
        <visual>
            <geometry>
                <cylinder length="0.008" radius="0.036"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
    </link>

    <joint name="base_to_left_wheel" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0 0.179 0" rpy="-1.57 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="head">
        <visual>
            <geometry>
                <box size="0.03 0.03 0.03"/>
            </geometry>
            <material name="blue">
                <color rgba="0 0 1 0.5"/>
            </material>
        </visual>
    </link>

    <joint name="base_to_head" type="fixed">
        <parent link="base_link"/>
        <child link="head"/>
        <origin xyz="0.159 0 0.027" rpy="0 0 0"/>
    </joint>

    <link name="passive_wheel_holder">
        <visual>
            <geometry>
                <cylinder length="0.012" radius="0.012"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
    </link>

    <joint name="base_to_passive_wheel_holder" type="fixed">
        <parent link="base_link"/>
        <child link="passive_wheel_holder"/>
        <origin xyz="0.162 0 -0.018" rpy="0 0 0"/>
    </joint>

    <link name="passive_wheel">
        <visual>
            <geometry>
                <sphere radius="0.012"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
    </link>

    <joint name="passive_wheel_holder_to_passive_wheel" type="fixed">
        <parent link="passive_wheel_holder"/>
        <child link="passive_wheel"/>
        <origin xyz="0 0 -0.006" rpy="0 0 0"/>
    </joint>

    <link name="left_wheel_indicator_1">
        <visual>
            <geometry>
                <box size="0.01 0.01 0.001"/>
            </geometry>
            <material name="cyan">
                <color rgba="0 1 1 1"/>
            </material>
        </visual>
    </link>

    <joint name="left_wheel_to_left_wheel_indicator_1" type="fixed">
        <parent link="left_wheel"/>
        <child link="left_wheel_indicator_1"/>
        <origin xyz="0 -0.031 0.0045" rpy="0 0 0"/>
    </joint>

    <link name="left_wheel_indicator_2">
        <visual>
            <geometry>
                <box size="0.01 0.01 0.001"/>
            </geometry>
            <material name="cyan">
                <color rgba="0 1 1 1"/>
            </material>
        </visual>
    </link>

    <joint name="left_wheel_to_left_wheel_indicator_2" type="fixed">
        <parent link="left_wheel"/>
        <child link="left_wheel_indicator_2"/>
        <origin xyz="0.0268 0.0155 0.0045" rpy="0 0 2.09"/>
    </joint>

    <link name="left_wheel_indicator_3">
        <visual>
            <geometry>
                <box size="0.01 0.01 0.001"/>
            </geometry>
            <material name="cyan">
                <color rgba="0 1 1 1"/>
            </material>
        </visual>
    </link>

    <joint name="left_wheel_to_left_wheel_indicator_3" type="fixed">
        <parent link="left_wheel"/>
        <child link="left_wheel_indicator_3"/>
        <origin xyz="-0.0268 0.0155 0.0045" rpy="0 0 -2.09"/>
    </joint>

    <link name="right_wheel_indicator_1">
        <visual>
            <geometry>
                <box size="0.01 0.01 0.001"/>
            </geometry>
            <material name="cyan">
                <color rgba="0 1 1 1"/>
            </material>
        </visual>
    </link>

    <joint name="right_wheel_to_right_wheel_indicator_1" type="fixed">
        <parent link="right_wheel"/>
        <child link="right_wheel_indicator_1"/>
        <origin xyz="0 -0.031 -0.0045" rpy="0 0 0"/>
    </joint>

    <link name="right_wheel_indicator_2">
        <visual>
            <geometry>
                <box size="0.01 0.01 0.001"/>
            </geometry>
            <material name="cyan">
                <color rgba="0 1 1 1"/>
            </material>
        </visual>
    </link>

    <joint name="right_wheel_to_right_wheel_indicator_2" type="fixed">
        <parent link="right_wheel"/>
        <child link="right_wheel_indicator_2"/>
        <origin xyz="0.0268 0.0155 -0.0045" rpy="0 0 2.09"/>
    </joint>

    <link name="right_wheel_indicator_3">
        <visual>
            <geometry>
                <box size="0.01 0.01 0.001"/>
            </geometry>
            <material name="cyan">
                <color rgba="0 1 1 1"/>
            </material>
        </visual>
    </link>

    <joint name="right_wheel_to_right_wheel_indicator_3" type="fixed">
        <parent link="right_wheel"/>
        <child link="right_wheel_indicator_3"/>
        <origin xyz="-0.0268 0.0155 -0.0045" rpy="0 0 -2.09"/>
    </joint>
</robot>